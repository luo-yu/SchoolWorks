[?1l>[?12l[?25h[?1049l]0;ylu236@ylu236: ~/fall2015/cs485g/labs/lab5ylu236@ylu236:~/fall2015/cs485g/labs/lab5$ cat fork.c 

#include <stdio.h>
#include <unistd.h>


main() {

  int x = 1;
  pid_t pid;

  
  pid = fork();

  if (pid ==0) {
    x++;
    printf("hello from child\n");
    printf("The process ID in Child = %d\n", getpid());
  }
  else {
    x--;
    sleep(1);
    printf("The process ID in Parent =%d\n", getpid());
    printf("The process ID of the Child = %i\n", pid);
    printf("hello from parent\n");
  }

}
