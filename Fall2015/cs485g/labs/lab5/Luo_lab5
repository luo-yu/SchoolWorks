[?1l>[?12l[?25h[?1049l]0;ylu236@ylu236: ~/fall2015/cs485g/labs/lab5ylu236@ylu236:~/fall2015/cs485g/labs/lab5$ cat fork.c 

#include <stdio.h>
#include <unistd.h>


main() {

  int x = 1;
  pid_t pid;

  
  pid = fork();

  if (pid ==0) {
    x++;
    printf("hello from child\n");
  }
  else {
    x--;
    sleep(1);
    printf("hello from parent\n");
  }

}
